@import url('https://fonts.googleapis.com/css?family=Indie+Flower');
  
  
.Cell_container{
    display: flex;
    align-items: flex-start;
    flex-wrap: wrap;

    width: 600px;
    height: 600px;
}

.Cell {
    border: 1px solid;
    box-sizing: border-box;
    width: 200px;
    height: 200px;
  
    display: flex;
    justify-content: center;
    flex-direction: column;
    text-align: center;
  }

.Piece{
    font-size: 100px;
    font-family: 'Indie Flower', cursive;
}



#svg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
}

#line{
  stroke-width:2px;
  stroke:rgb(0,0,0);
}